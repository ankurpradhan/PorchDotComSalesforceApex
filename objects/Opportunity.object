    <fields>
        <fullName>Days_Passed_Close_Date__c</fullName>
        <description>Calculates the number of days delinquent an opportunity is.</description>
        <externalId>false</externalId>
        <formula>if(
     AND( 
             OR(
                 ispickval(StageName, &apos;Close Won&apos;) = FALSE           
                ,ispickval(StageName, &apos;Close Lost&apos;) = FALSE
                )
         ,(TODAY() &gt; CloseDate)
        )
,TODAY() - CloseDate
,0
)</formula>
        <label>Days Passed Close Date</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
