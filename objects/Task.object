    <validationRules>
        <fullName>NVMContactWorld__Interac_Quality_Must_Be_Between_1_and_10</fullName>
        <active>true</active>
        <errorConditionFormula>NVMContactWorld__InteractionQuality__c &lt; 1  ||  NVMContactWorld__InteractionQuality__c &gt; 10</errorConditionFormula>
        <errorMessage>Interaction Quality must be a number between 1 and 10</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Task_Subtype_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Type, &quot;Cancellation Management Series&quot;), 
ISPICKVAL( Task_Subtype__c,&quot;&quot; ), 
NOT( 
OR(($Profile.Id = &quot;00ej0000000uw9O&quot;), /*System Administrator*/ 
($Profile.Id = &quot;00ej00000016bYW&quot;) /*System Administrator No Password*/ 
) 
))</errorConditionFormula>
        <errorDisplayField>Task_Subtype__c</errorDisplayField>
        <errorMessage>Hey there! This looks like part of the cancellation management series, please select the number of call in the series.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Warm_Pass_by_Field_Required</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Type, &quot;Warm pass&quot;), 
ISBLANK(Warm_Pass_by__c), 
NOT( 
OR(($Profile.Id = &quot;00ej0000000uw9O&quot;), /*System Administrator*/ 
($Profile.Id = &quot;00ej00000016bYW&quot;) /*System Administrator No Password*/ 
) 
))</errorConditionFormula>
        <errorDisplayField>Warm_Pass_by__c</errorDisplayField>
        <errorMessage>Hey there! This looks like a warm pass, if you want credit (which we know you do) populate this field with your name!</errorMessage>
    </validationRules>
